{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Focuh Documentation",
  "siteUrl": "https://focuh.com/docs",
  "colors": {
    "primary": "#F97316",
    "light": "#FB923C",
    "dark": "#EA580C"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Getting Started",
            "pages": [
              "index",
              "quickstart",
              "changelog"
            ]
          },
          {
            "group": "Features",
            "pages": [
              "features/focus-sessions",
              "features/distraction-blocking",
              "features/calendar",
              "features/leaderboards"
            ]
          },
          {
            "group": "Integrations",
            "pages": [
              "integrations/mcp"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "App",
          "href": "https://focuh.com",
          "icon": "rocket"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@focuh.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Get Focuh",
      "href": "https://focuh.com"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "cursor"
    ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/focuhapp"
    }
  }
}
